[DEFAULT]
log_level = DEBUG
files = ../linux*/fs/configfs/*.c
bears = ClangCloneDetectionBear
condition_list =
  returned,
  is_condition,
  in_condition,
  in_second_level_condition,
  in_third_level_condition,
  is_assignee,
  is_assigner,
  loop_content,
  second_level_loop_content,
  third_level_loop_content,
  is_param,
  in_sum,
  in_product,
  in_binary_operation

[FS]
enabled = false
files = ../linux*/fs/**/*.c

[BFS]
# Benchmarking FS
bear_dirs = ./src/**
bears = CloneDetectionBenchmarkBear
