[DEFAULT]
enabled = false
log_level = DEBUG
bear_dirs = ./src/**
condition_list = used: 0.5, returned, is_condition, in_condition, is_assignee, is_assigner, loop_content

[PRINT]
bears = ClangASTPrintBear
files = ./samples/*.c

[NOT]
enabled = true
files = ./samples/tests/not_clones/*.c
bears = ClangCloneDetectionBear

[CLONES]
enabled = true
files = ./samples/tests/clones/*.c
bears = ClangCloneDetectionBear
